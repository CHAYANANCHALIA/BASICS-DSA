import java.util.*;
class HelloWorld {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int number = sc.nextInt();
         System.out.print(number+ " is ");
        String type = (number<0)?"negative":"positive";
         System.out.print(type);
        
    }
}
