import java.util.*;
public class PrintingSentenceVariable{
    public static void main(String args[]){
        Scanner sc = new Scanner(System.in);
        int a = sc.nextInt();
        System.out.print("you gave" +  (a) + "as input");//no space
    System.out.print("\nyou gave " +  (a) + " as input ");/*with space*/
    }
}
