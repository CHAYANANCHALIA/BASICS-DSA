import java.util.*;
public class reverse{
    public static void main(String args[]){
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int reverse = 0;
        int lastdigit;
        while(n>0)
        lastdigit = n%10;
        reverse = (reverse*10)+lastdigit;
         System.out.print();
         n = n/10;
    }
     System.out.print(reverse);
}
