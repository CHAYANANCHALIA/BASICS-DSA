//class name A&B X
public class SumOfAB{
    public static void main(String args[]){
        int a=10;
        int b=17;
        int sum=a+b;/*Impressive programmers give logical variable names*/
        System.out.print(sum);
        System.out.println(a+b);
        System.out.println(a=b);
        System.out.println(a);
    }
}
