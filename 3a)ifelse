import java.util.*;
public class IFELSE{
    public static void main(String args[]){
        Scanner sc = new Scanner(System.in);
        int age = sc.nextInt();
        if (age>=18){
            System.out.println("Congrats you can be tried in court");
        }
        else if (age<18 && age>=16){
            System.out.println("You can be a total criminal and half a prisoner");
        }
        else{
        System.out.println("if you are a criminal now you must be a psycho");
        
        }
    }
}
