import java.util.*;
public class breverse{
    public static void main(String args[]){
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int reverse = 0;
        int lastdigit;
        while(n>0){
        lastdigit = n%10;
        reverse = (reverse*10)+lastdigit;
      
         n = n/10;

        }
        System.out.println(reverse);
    }
     
}
